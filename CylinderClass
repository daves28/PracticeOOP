class Cylinder:
    
    pi = 3.14
    
    def __init__(self,height=1,radius=1):
        
        self.height = height
        self.radius = radius
        
    def volume(self):
        
        return self.radius*self.radius*self.pi*self.height
    
    def surface_area(self):
        
        return 2*self.pi*self.radius*self.height + 2*self.pi*self.radius**2
        
# EXAMPLE OUTPUT
c = Cylinder(2,3)

c.volume()
#56.52

c.surface_area()
#94.2
